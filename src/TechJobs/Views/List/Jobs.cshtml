<h1>@ViewBag.title</h1>
@{
// TODO #3 - Use the same list display from Views/Search/Index.cshtml
    @if (ViewBag.jobs != null)
    {

        <h3>@ViewBag.jobs.Count Result(s)</h3>

        // TODO #2 - Loop over jobs dictionary and display all job fields

        @foreach (Dictionary<string, string> perJob in ViewBag.jobs)
        {   <table class="job-listing">
        <thread>
            @foreach (KeyValuePair<string, string> i in perJob)
            {
                <tr><th>@i.Key  &nbsp;&nbsp;&nbsp;@i.Value</th></tr>

            }
        </thread>
    </table>
        }
    }
}